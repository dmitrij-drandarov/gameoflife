<?xml version="1.0" encoding="$encoding"?>
<jnlp
    spec="$jnlpspec"
      codebase="http://localhost:8123/files"
    href="$outputFile">
  <information>
    <title>Game of Life</title>
    <vendor>Tino Steinort</vendor>
    <homepage href="https://github.com/tinosteinort/gameoflife"/>
#if($project.Description)
    <description>$project.Description</description>
#end
#if($offlineAllowed)
    <offline-allowed/>
#end
  </information>
#if($allPermissions)
  <security>
     <sandbox/>
  </security>
#end
  <resources>
    <j2se version="$j2seVersion"/>
     $dependencies
     $extensions
  </resources>
  <application-desc main-class="$mainClass">
      $arguments
  </application-desc>
</jnlp>

